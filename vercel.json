{
    "version": 2,
    "builds": [{
        "src": "ecommerce/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "50mb", "runtime": "python3.9" }
    }],
    "routes": [{
        "src": "build_files.sh",
        "dest": "ecommerce/wsgi.py"
    }],
    "env": {
        "SECRET_KEY": "@your-secret-key",
        "DATABASE_URL": "@your-database-url"
    }
}